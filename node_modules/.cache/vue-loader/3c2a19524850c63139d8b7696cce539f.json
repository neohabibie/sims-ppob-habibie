<template v-slot:button-content>\r\n            <span class=\"nav-link text-dark\">Akun</span>\r\n          </template>\r\n          <b-dropdown-item @click=\"$router.push('/profile')\">Profile</b-dropdown-item>\r\n          <b-dropdown-item @click=\"logout\" class=\"text-danger\">Logout</b-dropdown-item>\r\n        </b-nav-item-dropdown>\r\n      </b-navbar-nav>\r\n    </b-collapse>\r\n  </b-navbar>\r\n</template>\r\n\r\n<script>\r\nimport api from \"../services/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      profile: {},\r\n      balance: 0,\r\n    };\r\n  },\r\n\r\n  computed: {\r\n    avatar() {\r\n      return this.profile.profile_image\r\n        ? this.profile.profile_image\r\n        : require(\"../assets/default-avatar.png\");\r\n    },\r\n\r\n    balanceDisplay() {\r\n      return new Intl.NumberFormat(\"id-ID\", {\r\n        style: \"currency\",\r\n        currency: \"IDR\",\r\n      }).format(this.balance || 0);\r\n    },\r\n  },\r\n\r\n  async created() {\r\n    try {\r\n      const [p, b] = await Promise.all([\r\n        api.get(\"/profile\"),\r\n        api.get(\"/balance\"),\r\n      ]);\r\n      this.profile = p.data.data || {};\r\n      this.balance =\r\n        b.data.data && b.data.data.balance ? b.data.data.balance : 0;\r\n    } catch (e) {\r\n      console.warn(\"navbar load:\", e);\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    logout() {\r\n      this.$store.dispatch(\"auth/logout\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.logo-sm {\r\n  width: 28px;\r\n  height: 28px;\r\n}\r\n</style>\r\n"]}]}